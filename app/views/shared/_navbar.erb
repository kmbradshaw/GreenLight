<nav>
<%= link_to root_path do %>
	<%= image_tag("logo.png", class: "icon") %>
	<h3 id="logo">GreenLight</h3>
<% end %>
  <% if !logged_in? %>
  	<% if current_page?(root_path) %>
      <%= link_to "Login", login_path, class:"nav-links" %>
      <%= link_to "Register", new_user_path, class:"nav-links" %>
    <% elsif current_page?(login_path) %>
      <%= link_to "Register", new_user_path, class:"nav-links" %>
    <% elsif current_page?(new_user_path) %>
    	<%= link_to "Login", login_path, class:"nav-links" %>
    <% end %>
  <% else %>
  		<% if current_page?('/dashboard') %>
	      <%= link_to "Log Out", logout_path, method: "delete", class:"nav-links" %>
      <% else %>
	      <%= link_to "Log Out", logout_path, method: "delete", class:"nav-links" %>
	      <%= link_to "Dashboard", '/dashboard', class: "nav-links"%>
      <% end %>
   <% end %>
</nav> 